print(1 >= 0); print("\n"); # true
print(0 >= 1); print("\n"); # false
print(1 >= 1); print("\n"); # true

print(1 <= 0); print("\n"); # false
print(0 <= 1); print("\n"); # true
print(1 <= 1); print("\n"); # true

print(1 > 0); print("\n"); # true
print(0 > 1); print("\n"); # false
print(1 > 1); print("\n"); # false

print(1 < 0); print("\n"); # false
print(0 < 1); print("\n"); # true
print(1 < 1); print("\n"); # false

print(1 == 0); print("\n"); # false
print(0 == 1); print("\n"); # false
print(1 == 1); print("\n"); # true

print(1 != 0); print("\n"); # true
print(0 != 1); print("\n"); # true
print(1 != 1); print("\n"); # false
