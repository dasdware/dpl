type point := [ x: number, y: number ];

function add(a: point, b: point): point 
    := [ x: a.x + b.x, y: a.y + b.y ];

function print(p: point): string
    := print("[x: " + p.x.to_string() + ", y: " + p.y.to_string() + "]");

var p1 := [ x: 1, y: 2 ];
print(p1); print("\n");
var p2: point := [ y: 2*3, x: 1 ];
print(p2); print("\n");
print(p1 + p2); print("\n");