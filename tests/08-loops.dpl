function fibonacci(n: number) :=
    if (n <= 1)
        n
    else {
        var fib := 1;
        var prevFib := 1;
        var i := 2;
        while (i < n) {
            var temp := fib;
            fib := fib + prevFib;
            prevFib := temp;
            i := i + 1;
        };
        fib
    };

var n: number := 1;
while (n < 20) {
    print(fibonacci(n)); print("\n");    
    n := n + 1;
}